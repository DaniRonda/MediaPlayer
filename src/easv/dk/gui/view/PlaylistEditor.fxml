<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="155.0" prefWidth="349.0" style="-fx-background-color: #faa7b0;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="easv.dk.gui.controller.PlaylistEditor">
   <children>
      <TextField fx:id="txtNewPlaylist" layoutX="110.0" layoutY="60.0" prefHeight="30.0" prefWidth="201.0" style="-fx-background-radius: 50;" />
      <Text fill="#000000b0" layoutX="60.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
         <font>
            <Font name="Calibri Light" size="14.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Text>
      <Button fx:id="saveNewPlaylist" layoutX="221.0" layoutY="109.0" mnemonicParsing="false" onAction="#saveNewPlaylist" style="-fx-background-radius: 50;" text="Save" textFill="#000000b0">
         <effect>
            <Glow />
         </effect>
      </Button>
      <Button fx:id="CloseBotton" layoutX="272.0" layoutY="109.0" mnemonicParsing="false" onAction="#cancelPlaylist" style="-fx-background-radius: 50;" text="Cancel" textFill="#000000b0">
         <effect>
            <Glow />
         </effect>
      </Button>
   </children>
</AnchorPane>
